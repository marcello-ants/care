{
  "name": "enrollment-mfe",
  "version": "0.871.0",
  "private": true,
  "scripts": {
    "amplitudeEventGenerate": "ampli pull -p src/__generated__/ampli",
    "build": "next build && tsc --project tsconfig.server.json",
    "debug": "env-cmd -f 'local.env' --use-shell \"NODE_OPTIONS='--inspect' ts-node --project tsconfig.server.dev.json src/server/index.ts\"",
    "carelocal": "docker run --rm --name=\"enrollment-mfe-proxy\" --pull=always -p 443:443 -e LOCAL_SERVICE_PATH=\"/app/enrollment\" care-docker-bronze-local.artifactory.gold.mgmt.carezen.net/docker/nginx-care-local:1",
    "dev": "env-cmd -f 'local.env' ts-node --project tsconfig.server.dev.json src/server/index.ts",
    "dev:carelocal": "concurrently yarn:carelocal yarn:dev",
    "lint": "eslint --ext js,ts,tsx . && yarn type-check",
    "lint:quiet": "eslint --quiet --ext js,ts,tsx . && yarn type-check",
    "format": "prettier \"src/**/*.{js,ts,tsx}\" --write",
    "start": "env-cmd -f 'local.env' --use-shell \"NODE_ENV=production node dist/server/index.js\"",
    "start:production": "node dist/server/index.js",
    "test": "jest",
    "test:all": "yarn lint && yarn test",
    "test:no-cover": "jest --coverage=false",
    "type-check": "tsc --pretty --noEmit",
    "typesTypeGen": "apollo client:codegen --graph=caredotcom-federated --variant=prod --passthroughCustomScalars --customScalarsPrefix=globalScalar --outputFlat --target typescript src/__generated__",
    "typesTypeGenLocal": "apollo client:codegen --endpoint=http://localhost:8081/api/graphql --passthroughCustomScalars --customScalarsPrefix=globalScalar --outputFlat --target typescript src/__generated__"
  },
